{
  "name": "frida-agent-example",
  "version": "1.0.0",
  "description": "Example Frida agent written in TypeScript",
  "private": true,
  "main": "agent/index.ts",
  "type": "module",
  "scripts": {
    "build": "esbuild agent/index.ts --minify --bundle --platform=node --outfile=_agent.js --sourcemap=inline",
    "inject": "esbuild agent/index.ts --minify --bundle --platform=node --outfile=_agent.js --sourcemap=inline && frida -U -n Calculator -l _agent.js"
  },
  "devDependencies": {
    "@types/frida-gum": "^19.0.0",
    "frida-java-bridge": "^7.0.1",
    "@types/node": "^22.15.29",
    "esbuild": "^0.25.5"
  }
}
